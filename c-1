#include <stdio.h>
#include <stdlib.h>
#include <time.h>

int main(){
  int w;
  printf("Podaj liczbe wierszy jaka chcesz wyswietlic: ");
  scanf("%d",&w);
  
  int rows=w;
  int i,j;
  int x;
  for (i=1;i<=rows;i++)
  {
    for(j=1;j<=i;j++)
      printf("%d ",x++);
    printf("\n");
  }
  printf("\n");
  return 0;
}
